{
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "clean": "turbo run clean",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "docker": "turbo prune --scope=abp-react --docker",
        "generate": "plop",
        "gen-api": "openapi --input https://abp.antosubash.com/swagger/v1/swagger.json --output ./packages/proxy/src --client axios NODE_TLS_REJECT_UNAUTHORIZED=0",
        "test:codegen": "pnpm exec playwright codegen"
    },
    "devDependencies": {
        "dotenv-cli": "latest",
        "eslint": "^8.38.0",
        "eslint-config-custom": "workspace:*",
        "openapi-typescript-codegen": "^0.23.0",
        "plop": "^3.1.2",
        "prettier": "^2.8.7",
        "prettier-plugin-tailwindcss": "^0.2.7",
        "turbo": "^1.9.1"
    },
    "packageManager": "pnpm@7.15.0"
}
